--
-- Name: home_select(integer, integer); Type: FUNCTION; Schema: public; Owner: @dminvincul@cion2022
--

CREATE FUNCTION public.home_select(id_type integer, id_parameter integer) RETURNS TABLE(status integer, infor text)
    LANGUAGE plpgsql
    AS $$
DECLARE
  status int:= 4;
  BEGIN
  	BEGIN
	-- COUNTS
		IF (id_type = 1) THEN

			status:= 2;

			return query select
			status, (select COALESCE(array_to_json(array_agg(row_to_json(home.*))),'[]') as home from (
			select

			(select count(*) as my_courses from persons_courses
			                               inner join courses c on c.id_course = persons_courses.courses_id_course
			 where persons_courses.persons_id_person = id_parameter and c.state_course = 'A' and persons_courses.state_person_course = 'A'),

			(select COALESCE(sum(persons_evaluations.qualification_person_evaluation),'0') as my_point from persons_evaluations
			where persons_id_person = id_parameter),

			(select count(*) as total_course from courses
			where state_course not in ('I')),

			(select count(*) as total_user from persons
			where type_person not in ('I', 'S'))
			) as home)::text;
			
		  ELSIF (id_type = 2) THEN

			status:= 2;

			return query select
			status, (select COALESCE(array_to_json(array_agg(row_to_json(home.*))),'[]') as home from (
			select

			(select count(*) as my_courses from courses
			 where persons_id_person = id_parameter),

			(select COALESCE(sum(persons_evaluations.qualification_person_evaluation),'0') as my_point from persons_evaluations
			where persons_id_person = id_parameter),

			(select count(*) as total_course from courses
			where state_course not in ('I')),

			(select count(*) as total_user from persons
			where type_person not in ('I', 'S'))
			) as home)::text;

		END IF;


	EXCEPTION WHEN OTHERS THEN
		raise notice '% %', SQLERRM, SQLSTATE;
		status:=4;
		rollback;
	END;
END;
$$;


