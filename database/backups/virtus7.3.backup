                                                       ) as answers_)::json
			                       from questions 
			                       inner join question_category qc on questions.question_category_id_questioncategory = qc.id_questioncategory
			                       where questions.evaluations_id_evaluation = evaluations.id_evaluation and state_question = 'A'
			                       order by question_category_id_questioncategory, order_question
			                       limit numberquestion_evaluation
			                       ) as questions_)::json
				FROM evaluations
