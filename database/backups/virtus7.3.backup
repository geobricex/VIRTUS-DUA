--

CREATE FUNCTION public.course_syllabu_topic_select(id_parameter_course bigint) RETURNS TABLE(information text)
    LANGUAGE plpgsql
    AS $$
DECLARE
  BEGIN
  	BEGIN
    -- SELECT information FROM course_syllabu_topic_select(1)
    -- select * from courses;
			return query select (select COALESCE(array_to_json(array_agg(row_to_json(courses_.*))),'[]') as courses_
			 from (
				   SELECT name_course, description_course, pathimg_course, language_course, price_course, datereg_course,
				          CONCAT(p.name_person, ' ', p.lastname_person) as fullname_person,
				       (select COALESCE(array_to_json(array_agg(row_to_json(syllabus_.*))),'[]') as syllabus_
			               from (  select name_syllabu, description_syllabu, pathimg_syllabus, datereg_syllabu,
			                              (select COALESCE(array_to_json(array_agg(row_to_json(topics_.*))),'[]') as topics_
                                              from (  select name_topic, description_topic, pathimg_topic, datereg_topic
                                                       from topics
                                                       where topics.syllabus_id_syllabu = syllabus.id_syllabu and state_topic = 'A'
                                                       order by datereg_topic
                                                       ) as topics_)::json
			                       from syllabus
			                       where syllabus.courses_id_course = courses.id_course and state_syllabu = 'A'
			                       order by datereg_syllabu
			                       ) as syllabus_)::json
				FROM courses
				INNER JOIN persons p on courses.persons_id_person = p.id_person
			    WHERE id_course = id_parameter_course

		) as courses_)::text;

	EXCEPTION WHEN OTHERS THEN
		raise notice '% %', SQLERRM, SQLSTATE;
		rollback;
	END;
END;
$$;


