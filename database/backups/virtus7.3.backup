                                                inner join courses c on s.courses_id_course = c.id_course
                                       where p.persons_id_person = id_personParam
                                       order by p.id_person_evaluation desc) as evaluation_person)::text;
             ELSIF (type = 3) THEN
            return query select (select COALESCE(array_to_json(array_agg(row_to_json(evaluation_person.*))),
                                                 '[]') as evaluation_person
                                 from (select p.id_person_evaluation,
                                              p.qualification_person_evaluation,
                                              p.persons_id_person,
                                              p.evaluations_id_evaluation,
                                              p.timespent_person_evaluation,
                                              p.trynumber_person_evaluation,
                                              p.datereg_person_evaluation,
                                              e.name_evaluation,
                                              e.type_evaluation,
                                              t.name_topic,
                                              s.name_syllabu,
                                              c.name_course
                                       from persons_evaluations as p
                                                inner join evaluations e on p.evaluations_id_evaluation = e.id_evaluation
                                                inner join topics t on e.topics_id_topic = t.id_topic
                                                inner join syllabus s on t.syllabus_id_syllabu = s.id_syllabu
                                                inner join courses c on s.courses_id_course = c.id_course
                                       where p.persons_id_person = id_personParam
                                       order by p.id_person_evaluation desc) as evaluation_person)::text;
             ELSIF (type = 4) THEN
             return query select (select COALESCE(array_to_json(array_agg(row_to_json(evaluation_person.*))),
                                                 '[]') as evaluation_person
                                 from (select p.id_person_evaluation,
